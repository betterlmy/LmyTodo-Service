    "definitions": {
        "api.BatchSyncRequest": {
            "type": "object",
            "properties": {
                "categories": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/repository.CategorySyncItem"
                    }
                },
                "settings": {
                    "$ref": "#/definitions/repository.UserSettingsSyncItem"
                },
                "todos": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/repository.TodoSyncItem"
                    }
                }
            }
        },
        "api.BatchSyncResponse": {
            "type": "object",
            "properties": {
                "conflicts": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/repository.SyncResult"
                    }
                },
                "errors": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/repository.SyncResult"
                    }
                },
                "success": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/repository.SyncResult"
                    }
                }
            }
        },
        "api.CategoryRequest": {
            "type": "object",
            "required": [
                "name"
            ],
            "properties": {
                "color": {
                    "type": "string",
                    "example": "#FF5722"
                },
                "icon": {
                    "type": "string",
                    "example": "work"
                },
                "name": {
                    "type": "string",
                    "example": "工作"
                }
            }
        },
        "api.DeleteCategoryRequest": {
            "type": "object",
            "required": [
                "id"
            ],
            "properties": {
                "id": {
                    "type": "integer",
                    "example": 1
                }
            }
        },
        "api.DeleteTodoRequest": {
            "type": "object",
            "required": [
                "id"
            ],
            "properties": {
                "id": {
                    "description": "TODO ID",
                    "type": "integer",
                    "example": 1
                }
            }
        },
        "api.ExtendedTodoRequest": {
            "type": "object",
            "required": [
                "title"
            ],
            "properties": {
                "category_id": {
                    "type": "integer",
                    "example": 1
                },
                "description": {
                    "type": "string",
                    "example": "学习Go语言基础语法和框架"
                },
                "due_date": {
                    "type": "string",
                    "example": "2023-12-31T23:59:59Z"
                },
                "priority": {
                    "type": "integer",
                    "example": 1
                },
                "reminder": {
                    "type": "string",
                    "example": "2023-12-30T09:00:00Z"
                },
                "tags": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    },
                    "example": [
                        "[\"工作\"",
                        "\"重要\"]"
                    ]
                },
                "title": {
                    "type": "string",
                    "example": "学习Go语言"
                }
            }
        },
        "api.GetTodosRequest": {
            "type": "object",
            "properties": {
                "limit": {
                    "type": "integer",
                    "example": 20
                },
                "offset": {
                    "type": "integer",
                    "example": 0
                }
            }
        },
        "api.IncrementalSyncRequest": {
            "type": "object",
            "properties": {
                "since": {
                    "type": "integer",
                    "example": 1640995200000
                }
            }
        },
        "api.LoginRequest": {
            "type": "object",
            "required": [
                "password",
                "username"
            ],
            "properties": {
                "password": {
                    "description": "密码",
                    "type": "string",
                    "example": "password123"
                },
                "username": {
                    "description": "用户名",
                    "type": "string",
                    "example": "admin"
                }
            }
        },
        "api.RegisterRequest": {
            "type": "object",
            "required": [
                "email",
                "password",
                "username"
            ],
            "properties": {
                "email": {
                    "description": "邮箱",
                    "type": "string",
                    "example": "user@example.com"
                },
                "password": {
                    "description": "密码",
                    "type": "string",
                    "example": "password123"
                },
                "username": {
                    "description": "用户名",
                    "type": "string",
                    "example": "newuser"
                }
            }
        },
        "api.Response": {
            "type": "object",
            "properties": {
                "code": {
                    "description": "响应码，0表示成功",
                    "type": "integer",
                    "example": 0
                },
                "data": {
                    "description": "响应数据，成功时包含具体数据",
                    "type": "object"
                },
                "message": {
                    "description": "响应消息",
                    "type": "string",
                    "example": "成功"
                }
            }
        },
        "api.SearchTodosRequest": {
            "type": "object",
            "required": [
                "keyword"
            ],
            "properties": {
                "keyword": {
                    "type": "string",
                    "example": "学习"
                },
                "limit": {
                    "type": "integer",
                    "example": 20
                },
                "offset": {
                    "type": "integer",
                    "example": 0
                }
            }
        },
        "api.SyncResponse": {
            "type": "object",
            "properties": {
                "categories": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/repository.CategorySyncItem"
                    }
                },
                "server_version": {
                    "type": "integer",
                    "example": 1640995200000
                },
                "settings": {
                    "$ref": "#/definitions/repository.UserSettingsSyncItem"
                },
                "todos": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/repository.TodoSyncItem"
                    }
                }
            }
        },
        "api.SyncVersionResponse": {
            "type": "object",
            "properties": {
                "version": {
                    "type": "integer",
                    "example": 1640995200000
                }
            }
        },
        "api.TodoRequest": {
            "type": "object",
            "required": [
                "title"
            ],
            "properties": {
                "description": {
                    "description": "任务描述",
                    "type": "string",
                    "example": "学习Go语言基础语法和框架"
                },
                "title": {
